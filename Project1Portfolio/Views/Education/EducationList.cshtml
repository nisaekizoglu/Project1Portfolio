@model List<Project1Portfolio.Models.Education>
@{
    ViewBag.Title = "EducationList";
    Layout = "~/Views/Admin/Index.cshtml";
}

<h2>Eğitimlerim Listesi</h2>

<table class="table table-bordered">
    <tr>
        <th>Id </th>
        <th>Eğitim Seviyesi </th>
        <th>Tarih </th>
        <th>Eğitim Yeri </th>
        <th>Açıklama </th>
        <th>Sil </th>
        <th>Güncelle </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EducationId </td>
            <td>@item.Title </td>
            <td>@item.EducationDate </td>
            <td>@item.SubTitle</td>
            <td>@item.Description</td>
            <td><a href="/Education/DeleteEducation/@item.EducationId" class="btn btn-danger">Sil</a> </td>
            <td><a href="/Education/UpdateEducation/@item.EducationId" class="btn btn-success">Güncelle</a> </td>
        </tr>
    }
</table>
<a href="/Education/CreateEducation/" class="btn btn-info">Yeni Eğitim Girişi</a>
